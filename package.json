{
  "name": "valentine-v2",
  "version": "1.0.0",
  "description": "Twitter bot powered by AI",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "dev:test": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts -- --test",
    "dev:verbose": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts -- --verbose",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "goat-x": "^1.1.0",
    "tough-cookie": "^4.1.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/tough-cookie": "^4.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
